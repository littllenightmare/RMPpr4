<Shell
    x:Class="pr4.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:pr4">
    <TabBar>
        <Tab Title="Ручное">
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}">
            </ShellContent>
        </Tab>
        <Tab Title="Автоматическое">
            <ShellContent ContentTemplate="{DataTemplate local:AutomaticPage}"/></Tab>
        <Tab Title="О программе">
            <ShellContent ContentTemplate="{DataTemplate local:Info}"/></Tab>
    </TabBar>
</Shell>
